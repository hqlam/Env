# Enable telnet: 1/iportal 'oPOD' 2/lui mso:POD 3/Advance > Remote Mgmt > enable Telnet
# telnet to XB3 Arris cpe u:p=[none]:arristi (or factory pwd from Kevin). Type "quit": to shell; type "exit": back to console; Exit out Ctrl }, Ctrl d
# telnet to XB3 cisco u:p=admin:POD; mainMenu>shell; tail -f /rdklogs/logs/WEBPAlog.txt.0
# telnet to XB6 Arris u:p=admin:POD
RDKlog="/rdklogs/logs/WEBPAlog.txt.0"
WAPlog="/rdklogs/logs/WEBPAlog.txt.0"
cpe_al () {      echo " alias cfg='cat /nvram/syscfg.db' && alias llr='ls -ltraF' && alias cdlr='cd /rdklogs/logs && llr' && alias tlr='tail  -f /rdklogs/logs/WEBPAlog.txt.0' && alias mi='ifconfig -a wan0' && alias catcfg='cat /nvram/webpa_cfg.json'  " ; }

# telnet works with both IPv4 and IPv6
telI () {      i=$1; i=${i:-$I2}; cpe_al;    telnet $i ; }
teli  () {     c=$2; c=${c:-basic}; hterm $c "telI  $1" ; }

