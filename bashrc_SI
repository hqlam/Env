### Green ODP APIs
GO2 () {       a="$1"; a=${a:-version};                open $grno/api/v2/operational/$a ; }

AO () {        eo=$1;  a="$2"; a=${a:-version};        open $eo/api/v1/operational/$a ; }

GO () {        a="$1"; a=${a:-version};                open $grno/api/v1/operational/$a ; }
YO () {        a="$1"; a=${a:-version};                AO   $yelo                    $a ; }

GOc () {       a="$1"; a=${a:-association};            open $grno$CSo1/$a ; }
GOm () {       a="$1"; a=${a:-association};            open $grno$CPEo1/$m/$a ;                       echo "assoN meshagent meshstatus xdns_content_filtering" ; }
GOd () {       m=$1; a=$2;                             open $grno$CPEo1/$m/devices/$a ;               echo "identity summary device_management_group_configuration device_management*" ; } 
GOddl () {     m=$1; dm=$2;                            open $grno$CPEo1/$m/devices?device_macs=$dm ;  }
GOddm () {     m=$1; dm=$2; a=$3;                      open $grno$CPEo1/$m/devices/$dm/$a ;           echo "summary; device_management_configuraN" ; }
GOtmo () {     m=$1; o=$2;                             open $grno/api/v1/operational/internal/gateway-cpe/$m/tunnel?names=$o ; echo "Device.X_Comcast_com_ParentalControl.ManagedDevices.Device." ; }

