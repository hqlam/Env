### LB, SparkM nodes, checks out of mem: 
# ean amber lb; for i in 96.119.228.186 96.119.244.139 96.118.23.63; do ech2 $i; df;  uptime; done
dfn () {       e=$1; a=$2; l=$3; for i in `ean $e $a $l|gc2 ip|a1line` ; do ech2 "===== $i:"; runcor $i "df;uptime;" ; done ; }
dfN () {       e=$1; a=$2; l=$3; for i in `eaN $e $a $l|gc2 ip|a1line` ; do ech2 "===== $i:"; runcor $i "df;uptime;" ; done ; }

topn () {      e=$1; a=$2; l=$3; for i in `ean $e $a $l|gc2 ip|a1line` ; do ech2 "===== $i:"; runcor $i "top -bn1|head;"  ; done ; }
topN () {      e=$1; a=$2; l=$3; for i in `eaN $e $a $l|gc2 ip|a1line` ; do ech2 "===== $i:"; runcor $i "top -bn1|head;"  ; done ; }


